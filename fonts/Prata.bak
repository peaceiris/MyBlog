/* 5. Se Didone: Prata */
@font-face {
    font-family: 'Prata';
    src:
        url('/fonts/Prata-Regular.ttf') format('truetype');
    font-style: normal;
    font-display: swap;
}
@font-face {
    font-family: 'Prata';
    src:
        url('/fonts/CrimsonPro-Italic-VF.ttf') format('truetype');
    font-weight: 1 1000;
    font-style: italic;
    font-display: swap;
}
@font-face {
  font-family: 'Fira Code VF';
  src: url('/fonts/FiraCode-VF.woff2') format('woff2-variations'),
       url('/fonts/FiraCode-VF.woff') format('woff-variations'),
       url('/fonts/FiraCode-VariableFont_wght.ttf') format('truetype');
    font-weight: 300 700;
    font-display: swap;
}
.Prata {
    --font-family-text: 'Prata', 'CooperHewitt', serif;
    --font-family-code: 'Fira Code VF', monospace;
}